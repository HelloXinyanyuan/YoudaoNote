<?xml version="1.0" encoding="utf-8"?>
<resources>
    <dimen name="activity_horizontal_margin">16dp</dimen>
    <dimen name="activity_vertical_margin">16dp</dimen>
    <dimen name="list_more_menu_1_height">200dp</dimen>
    <dimen name="list_more_menu_1_margin_right">@dimen/activity_horizontal_margin</dimen>
    <dimen name="list_more_menu_1_margin_top">20dp</dimen>
    <dimen name="list_more_menu_1_width">132dp</dimen>
    <string name="action_settings">Settings</string>
    <string name="app_name">YoudaoNote</string>
    <string name="hello">Hello World, XListViewActivity!</string>
    <string name="hello_world">Hello world!</string>
    <string name="pull_to_refresh_pull_label">Pull to refresh...</string>
    <string name="pull_to_refresh_refreshing_label">Loading...</string>
    <string name="pull_to_refresh_release_label">Release to refresh...</string>
    <string name="pull_to_refresh_tap_label">Tap to refresh...</string>
    <string name="xlistview_footer_hint_normal">查看更多</string>
    <string name="xlistview_footer_hint_ready">松开载入更多</string>
    <string name="xlistview_header_hint_loading">正在加载...</string>
    <string name="xlistview_header_hint_normal">下拉刷新</string>
    <string name="xlistview_header_hint_ready">松开刷新数据</string>
    <string name="xlistview_header_last_time">上次更新时间：</string>
    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
        <item name="android:windowNoTitle">true</item>
    </style>
    <style name="HomeTabTxtStyle">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/docker_tab_text_color_selector</item>
    </style>
    <style name="NoteTabTxtStyle" parent="@style/HomeTabTxtStyle">
        <item name="android:textColor">@color/note_tab_text_color_selector</item>
        <item name="android:textSize">18sp</item>
    </style>
    <style name="list_more_item">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">48dp</item>
        <item name="android:paddingLeft">12dp</item>
        <item name="android:gravity">left|center_vertical</item>
        <item name="android:textColor">#d6d6d6</item>
        <item name="android:textSize">16sp</item>
        <item name="android:drawablePadding">8dp</item>
    </style>
    <style name="list_more_item_divider">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1px</item>
        <item name="android:layout_marginTop">-1px</item>
        <item name="android:background">#222425</item>
    </style>
    <style name="setting_item_arrow">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:src">@drawable/arrow_right</item>
        <item name="android:layout_marginRight">@dimen/activity_horizontal_margin</item>
    </style>
    <style name="setting_item_divider">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1px</item>
        <item name="android:layout_marginTop">-1px</item>
        <item name="android:background">#cccccc</item>
    </style>
    <style name="setting_item_layout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:paddingLeft">@dimen/activity_horizontal_margin</item>
        <item name="android:background">@drawable/setting_item_selector</item>
    </style>
    <style name="setting_item_text">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#292929</item>
        <item name="android:textSize">16sp</item>
    </style>
</resources>